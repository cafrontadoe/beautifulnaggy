<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity UserClient.
    -->
    <changeSet id="20200121204054-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="type_doc_id"
                                 baseTableName="user_client"
                                 constraintName="fk_user_client_type_doc_id"
                                 referencedColumnNames="id"
                                 referencedTableName="type_doc"/>

        <addForeignKeyConstraint baseColumnNames="sale_id"
                                 baseTableName="user_client"
                                 constraintName="fk_user_client_sale_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sale"/>

    </changeSet>
</databaseChangeLog>
